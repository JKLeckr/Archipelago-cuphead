{
  "include": ["**/*.py"],

  "exclude": ["**/__pycache__"],

  "typeCheckingMode": "strict",
  "reportImplicitOverride": "error",
  "reportMissingImports": true,
  "reportDeprecated": "error",

  "deprecateTypingAliases": true,

  "pythonVersion": "3.10",
  "pythonPlatform": "Windows",

  "executionEnvironments": [
    {
      "root": "../.."
    }
  ]
}
